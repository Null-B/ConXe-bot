@client.event
async def on_kick_error(ctx, error, member : discord.Member):
    if isinstance(error, commands.MissingPermissions):
        embed = discord.Embed(
            title = f'{member.name} You cant do that',
            description = "no",
            color = discord.Color.red()
        )
        await ctx.send(embed=embed)

